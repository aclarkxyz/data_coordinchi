{
    "compilerOptions": 
    {
        "module": "system",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outFile": "app/coordinchi.js",
        "sourceMap": true,
        "target": "ES6",
		"incremental": true,
		"lib": ["es2017", "dom"],
        "types": []
    },
    "include": 
    [
        "src/**/*.ts",
        
        // imported from WebMolKit
        "../WebMolKit/src/**/*.ts"
    ],
    "exclude":
    [
        "node_modules",
        "dist"
    ]
}

